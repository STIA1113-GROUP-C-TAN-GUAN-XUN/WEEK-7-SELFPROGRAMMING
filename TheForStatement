package selfProgramming;

public class TheForStatement {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		for(int i =1; i<5; i++) {
			System.out.println(i);
		}
		
		for(int y=0; y<=5;y++) {
			System.out.println("The condition of loops is stil correct " +y);
			System.out.println("*****");
		}
		
		for (int x=0; x<=3; x++) {
			System.out.println("Yeet" +x);
		}
		System.out.println("No more Yeet");
		
		
		
	}
