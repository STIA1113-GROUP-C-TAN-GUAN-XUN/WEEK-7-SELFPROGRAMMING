package selfProgramming;

public class TheWhileStatement {
	
	public static void main (String [] args) {
		
		int i =1;
		
			while(i<=20) {
				System.out.println(i);
				i+=2;
			}
			
			int sum=0, number=1;
			
				while(number <=10) {
					sum+=number;
					number+=5;
					System.out.println("SUM: " + sum);
				}
				System.out.println("The sum is now greater than 10");
		
		
	}

}
